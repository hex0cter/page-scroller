{
  "manifest_version": 2,
  "name": "Smooth Scroll",
  "author": "Daniel Han",
  "short_name": "Smooth Scrolling",
  "description": "Scroll your webpage without moving your palm off the center of keyboard",
  "version": "1.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "activeTab",
   "storage",
   "tabs",
   "http://*/",
   "https://*/",
   "file:///*/"
   ]
}
